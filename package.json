{
  "name": "@withjoy/server-core",
  "version": "1.3.0",
  "description": "Joy Server Base and Utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "engines": {
    "node": ">=10 <11"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "prepublishOnly": "yarn test",
    "build": "tsc",
    "test": "NODE_ENV=test jest --detectOpenHandles --no-cache --forceExit --config jest.config.js",
    "gentype:local": "apollo client:codegen --endpoint=http://localhost:9091/graphql --target=typescript --includes=src/**/*.ts --outputFlat=src/graphql/generated.typings.ts",
    "gentype:dev": "apollo client:codegen --endpoint=https://bliss-gateway-staging.withjoy.com/graphql --target=typescript --includes=src/**/*.ts --outputFlat=src/graphql/generated.typings.ts",
    "prepare": "yarn build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "graphql": "14.4.2"
  },
  "dependencies": {
    "@types/cors": "2.8.4",
    "@types/express": "4.16.1",
    "@types/graphql": "^14.2.3",
    "@types/jest": "24.0.0",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.136",
    "@types/morgan": "1.7.35",
    "@types/node-fetch": "2.1.6",
    "@withjoy/telemetry": "^0.4.3",
    "apollo": "^2.16.2",
    "apollo-link": "1.2.8",
    "apollo-link-http": "1.5.11",
    "apollo-server": "^2.7.2",
    "apollo-server-express": "^2.7.2",
    "body-parser": "^1.18.3",
    "chroma-js": "^2.0.4",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "express": "4.16.4",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "immer": "^3.1.3",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.13",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "morgan": "1.9.1",
    "node-fetch": "2.3.0",
    "typeorm": "0.2.13",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/chroma-js": "^1.4.1",
    "@types/lolex": "^3.1.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/nock": "^10.0.2",
    "apollo-server-testing": "^2.7.2",
    "jest": "24.1.0",
    "lolex": "^4.0.1",
    "nock": "^10.0.6",
    "node-mocks-http": "^1.7.3",
    "ts-jest": "23.10.5",
    "typemoq": "2.1.0"
  }
}
